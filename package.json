{
  "name": "infinite-fleet-companion",
  "description": "A companion app for the Infinite Fleet game",
  "version": "0.0.2",
  "author": "Pixelmatic",
  "homepage": "https://www.infinitefleet.com",
  "engines": {
    "node": ">=16.13.1"
  },
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration dev-web",
    "build:prod:android": "npm run lint && npm run sync-platform-configs && ionic cap sync --platform android --configuration production",
    "build:prod:ios": "npm run lint && npm run sync-platform-configs && ionic cap sync --platform ios --configuration production",
    "build:prod": "npm run build:prod:android && npm run build:prod:ios",
    "build:dev": "npm run build:dev:android && npm run build:dev:ios",
    "build:dev:android": "npm run sync-platform-configs && ionic cap sync --platform android --configuration dev",
    "build:dev:ios": "npm run sync-platform-configs && ionic cap sync --platform ios --configuration dev",
    "open:ios": "ionic cap open ios",
    "open:android": "ionic cap open android",
    "open:android:device": "ionic capacitor run android --device",
    "sync-platform-configs": "node ./sync-platform-configs.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepare": "husky install",
    "proto:generate": "protoc --plugin=protoc-gen-ng=$(which protoc-gen-ng) --ng_out=./src/proto -I ./proto $(find ./proto -iname \"*.proto\")"
  },
  "dependencies": {
    "@angular/common": "13.3.9",
    "@angular/core": "13.3.9",
    "@angular/elements": "13.3.9",
    "@angular/forms": "13.3.9",
    "@angular/platform-browser": "13.3.9",
    "@angular/platform-browser-dynamic": "13.3.9",
    "@angular/router": "13.3.9",
    "@auth0/auth0-angular": "1.10.0",
    "@capacitor-community/appcenter": "3.0.0",
    "@capacitor-community/appcenter-analytics": "3.0.0",
    "@capacitor-community/appcenter-crashes": "3.0.1",
    "@capacitor-community/barcode-scanner": "^3.0.0",
    "@capacitor/android": "4.3.0",
    "@capacitor/app": "4.0.1",
    "@capacitor/browser": "4.0.1",
    "@capacitor/core": "4.3.0",
    "@capacitor/device": "4.0.1",
    "@capacitor/haptics": "4.0.1",
    "@capacitor/ios": "4.3.0",
    "@capacitor/keyboard": "4.0.1",
    "@capacitor/network": "4.0.1",
    "@capacitor/preferences": "4.0.1",
    "@capacitor/splash-screen": "4.1.0",
    "@capacitor/status-bar": "4.0.1",
    "@ionic/angular": "6.3.2",
    "@ngx-matomo/router": "2.6.0",
    "@ngx-matomo/tracker": "2.6.0",
    "@ngx-grpc/common": "^3.0.3",
    "@ngx-grpc/core": "^3.0.3",
    "@ngx-grpc/grpc-web-client": "^3.0.3",
    "@ngx-grpc/well-known-types": "^3.0.3",
    "@ngx-translate/core": "14.0.0",
    "@pixelmatic/ewk": "0.0.8",
    "@pixelmatic/inf-web-components": "0.1.3-dev-118",
    "bootstrap": "5.1.3",
    "browserify-fs": "1.0.0",
    "compare-versions": "4.1.3",
